<!--ユーザーの詳細-->
<div>
<h1>User info</h1>
<%= image_tag user.get_profile_image(100,100)%>
<tbody>
  <tr>
    <th>name<th>
    <th><%= user.name %></th>
  </tr>
  <tr>
    <th>introduction</th>
    <th><%= user.introduction %></th>
  </tr>
</tbody>
</div>

<!--新規投稿-->
<h1>New book</h1>
<%= form_with model: book do |f| %>
<h4>Title</h4>
<%= f.text_field :title %>
<h4>Opinion</h4>
<%= f.text_area :body %>

<%= f.submit 'Create Book' %>
<% end %>